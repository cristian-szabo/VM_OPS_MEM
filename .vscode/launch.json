{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug C++",
            "type": "cppdbg",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "program": "/bin/python3",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Monitor",
            "type": "debugpy",
            "request": "launch",
            "program": "monitor.py",
            "args": [
                // "--ops",
                /* MATRIX MULTIPLY ACCUMULATE */
                // "MMLA_S8_S32",
                // "MMLA_BF16_F32",
                /* MULTIPLY ACCUMULATE */
                // "MLA_F32_F32",
                // "MLA_BF16_F32",
                // "MLA_S8_S16",
                /* DOT PRODUCT */
                // "DOT_BF16_F32",
                // "DOT_S8_S32",
                /* FUSED MULTIPLY ACCUMULATE */
                // "FMA_F32_F32",
                // "FMA_F16_F16",
                // "FMA_F16_F32"
            ],
            "console": "integratedTerminal"
        }
    ]
}
